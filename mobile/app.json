{
  "expo": {
    "name": "Readsphere",
    "slug": "readsphere",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "readsphere",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.readsphere.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.readsphere.app",
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "resize"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "backgroundColor": "#0A0908",
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3940256099942544~3347511713",
          "iosAppId": "ca-app-pub-3940256099942544~1458002511"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0A0908",
          "dark": {
            "backgroundColor": "#0A0908"
          }
        }
      ],
      "expo-font",
      "expo-video",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow ReadSphere to access your camera to scan book barcodes."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow ReadSphere to access your photos to set profile pictures and share book covers.",
          "savePhotosPermission": "Allow ReadSphere to save images to your library.",
          "isAudioEnabled": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "owner": "vedchaudhari",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1751609a-939a-441a-be9b-9bb2dfe44221"
      }
    }
  }
}